Proceso ejercicio1
	
	Definir numero1 como Numerica;
	Escribir "Escriba un numero para determinar si tiene 3 digitos o no";
	Leer numero1;
	
	Si numero1 < 99 Entonces
		Escribir "El numero tiene menos de 3 digitos.";
	SiNo
		Si numero1 > 999 Entonces
				Escribir "El numero tiene mas de 3 digitos.";
		Sino
			Escribir "El numero tiene 3 digitos.";
		FinSi
	FinSi
	
